{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/sls offline start -r us-east-1 --noTimeout",
    "deploy": "sls deploy --verbose",
    "debug": "export SLS_DEBUG=* && node --inspect-brk=9229 ./node_modules/.bin/serverless offline start -r us-east-1 --noTimeout dev",
    "setup": "./node_modules/.bin/sls config credentials --provider aws --key KEY --secret SECRET",
    "install": "docker pull lambci/lambda",
    "install:dynamodb": "./node_modules/.bin/sls dynamodb install",
    "start:dynamodb": "./node_modules/.bin/sls dynamodb start -p 8000  --migrate true"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "moment": "^2.24.0",
    "serverless-dynamodb-client": "0.0.2",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.23",
    "@types/axios": "^0.14.0",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.3.7",
    "serverless": "^1.48.2",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.6.1",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^5.4.5",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
